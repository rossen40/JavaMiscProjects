<mvc:View height="100%" controllerName="DamageCalculator.controller.App" displayBlock="true" xmlns="sap.m" xmlns:tnt="sap.tnt"
	xmlns:mvc="sap.ui.core.mvc">
	<Shell>
		<App id="app">
			<tnt:ToolPage id="mainToolPage">
				<tnt:header>
				</tnt:header>
				<tnt:sideContent>
					<tnt:SideNavigation id="sideNavigation">
						<tnt:NavigationList>
							<tnt:NavigationListItem text="Wolfie">
								<tnt:NavigationListItem text="Wolf Calculator" select=".wolfCalcPress" />
								<tnt:NavigationListItem text="Wolf Calculator JSON" select=".wolfCalcJsonPress" />
								<tnt:NavigationListItem text="Wolf Calculator Result" select=".wolfResultPress"
									visible="{sharedModel>/visibilityWolfResult}" />
							</tnt:NavigationListItem>
							<tnt:NavigationListItem text="Battle">
<!-- 								<tnt:NavigationListItem text="Battle Calculator" select=".battleCalcPress" /> -->
								<tnt:NavigationListItem text="Battle Calculator JSON" select=".battleCalcJsonPress" />
								<tnt:NavigationListItem text="Battle Result" select=".battleResultPress"
									visible="{sharedModel>/visibilityBattleResult}" />
							</tnt:NavigationListItem>
						</tnt:NavigationList>
					</tnt:SideNavigation>
				</tnt:sideContent>
				<tnt:mainContents height="100%">
				</tnt:mainContents>
			</tnt:ToolPage>
		</App>
	</Shell>
</mvc:View>